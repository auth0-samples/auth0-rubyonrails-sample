var options = {
  auth: {
		redirectUrl: '<%= Rails.application.secrets.auth0_callback_url %>',
		params: {
			scope: 'openid name email picture'
		}
  }
};
var lock = new Auth0Lock('<%= Rails.application.secrets.auth0_client_id %>', '<%= Rails.application.secrets.auth0_domain %>',options);
function signin() {
	lock.show();
}
