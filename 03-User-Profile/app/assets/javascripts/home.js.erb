var lock = new Auth0Lock('<%= Rails.application.secrets.auth0_client_id %>', '<%= Rails.application.secrets.auth0_domain %>');
function signin() {
	lock.show({
	    callbackURL: '<%= Rails.application.secrets.auth0_callback_url %>',
	    responseType: 'code',
	    authParams: {
	    	scope: 'openid name email picture'
	  	}
	});
}
